<h1 class="uk-heading-divider">
    <%= link_to "", admin_user_path(@user), :"uk-icon" => "chevron-left", class: "uk-icon" %>     
    Editar Usuario
    <input type="submit" form="admin_user_form" value="Guardar" data-disable-with="Guardando..." class="uk-button uk-button-default uk-align-right uk-visible@s">
</h1>
<%= render 'form', user: @user %>

<%= link_to 'Show', [:admin, @user] %> |
<%= link_to 'Destroy', [:admin, @user], method: :delete, data: { confirm: 'Are you sure?' } %>

