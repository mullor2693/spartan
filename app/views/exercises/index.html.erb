<h1>Exercises</h1>

<%= link_to 'Back', authenticated_root_path %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exercises.each do |exercise| %>
      <tr>
        <td><%= exercise.name %></td>
        <td><%= link_to 'Show', exercise %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
