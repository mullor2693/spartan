<h1 class="uk-heading-divider"><%= link_to "", authenticated_root_path, :"uk-icon" => "chevron-left", class: "uk-icon" %> Entrenamiento</h1>

<div class="uk-child-width-1-3@m uk-grid-match" uk-grid>
    <% [
        ['Workouts', "Acceso a entrenamientos.", workouts_path],
        ['Ejercicios', "Acceso al listado de ejercicios.", exercises_path]
    ].each do |link_url| %>
        <div>
        <%= link_to link_url[2], class:"uk-card uk-card-hover uk-card-body uk-link-reset" do %>
            <h3 class="uk-card-title"><%= link_url[0] %></h3>
            <p><%= link_url[1] %></p>
        <% end %>
        </div>
    <% end %>
</div>